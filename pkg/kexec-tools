STATICBUILD=1
[deps]

[build]

CFLAGS="$optcflags" LDFLAGS="$optldflags -static" \
  ./configure -C --prefix="$butch_prefix" $xconfflags \
  --without-xen --sbindir="$butch_prefix"/bin \


make -j$MAKE_THREADS
make DESTDIR="$butch_install_dir" install
